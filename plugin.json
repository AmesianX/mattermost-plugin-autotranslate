{
    "id": "autotranslate",
    "name": "Autotranslate",
    "description": "Autotranslate plugin for Mattermost 5.3 and above.",
    "version": "0.1.0",
    "backend": {
        "executable": "server/plugin.exe"
    },
    "webapp": {
        "bundle_path": "webapp/main.js"
    },
    "settings_schema": {
        "settings": [
            {
                "key": "AWSAccessKeyID",
                "display_name": "AWS Access Key ID",
                "type": "text",
                "help_text": "The access key ID from AWS"
            },
            {
                "key": "AWSSecretAccessKey",
                "display_name": "AWS Secret Access Key",
                "type": "text",
                "help_text": "The secret access key from AWS"
            },
            {
                "key": "AWSRegion",
                "display_name": "AWS Region",
                "type": "text",
                "help_text": "The region from AWS"
            }
        ]
    }
}